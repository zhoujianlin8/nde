{
  "name": "<%=appname%>",
  "version": "0.0.1",
  "description": "koa2",
  "main": "app/index.js",
  "scripts": {
    "test": "mocha",
    "daily": "NODE_ENV=daily node_modules/.bin/pm2 kill && node_modules/.bin/pm2 start bin/server.js -i true",
    "pre": "NODE_ENV=pre node_modules/.bin/pm2 kill && node_modules/.bin/pm2 start bin/server.js -i true",
    "publish": "NODE_ENV=publish node_modules/.bin/pm2 kill && node_modules/.bin/pm2 start bin/server.js  -i true",
    "start": "NODE_ENV=local node bin/server.js",
    "debug": "NODE_ENV=local devdebug bin/server.js --watch={config,app}/{**/*,*}.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "koa2"
  ],
  "author": "",
  "bugs": {
    "url": ""
  },
  "engines": {
    "node": ">=7.6.0"
  },
  "dependencies": {
    "koa": "^2.3.0",
    "koa-body": "^2.3.0",
    "koa-ejs": "^4.1.0",
    "koa-logger": "^3.0.1",
    "koa-router": "^7.2.1",
    "koa-session": "^5.4.0",
    "merge": "^1.2.0",
    "pm2": "^2.6.1"
  },
  "devDependencies": {
    "mocha": "^3.1.0",
    "should": "^11.1.0"
  },
  "license": "MIT"
